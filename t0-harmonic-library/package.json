{
    "name":  "t0-harmonic-library",
    "version":  "2.0.0",
    "description":  "Advanced difference tone-based chord analysis and reconstruction system - Part of T0-Time-Mass-Duality research by Johann Pascher",
    "main":  "dist/t0-audio-system.cjs.js",
    "module":  "dist/t0-audio-system.esm.js",
    "browser":  "dist/t0-audio-system.umd.js",
    "types":  "dist/types/index.d.ts",
    "files":  [
                  "dist/",
                  "src/",
                  "docs/",
                  "examples/",
                  "README.md",
                  "LICENSE"
              ],
    "scripts":  {
                    "build":  "rollup -c rollup.config.js",
                    "build:dev":  "rollup -c rollup.config.js --environment NODE_ENV:development",
                    "build:prod":  "rollup -c rollup.config.js --environment NODE_ENV:production",
                    "test":  "jest",
                    "test:watch":  "jest --watch",
                    "test:coverage":  "jest --coverage",
                    "lint":  "eslint src/**/*.js",
                    "lint:fix":  "eslint src/**/*.js --fix",
                    "docs":  "jsdoc src/ -d docs/api/",
                    "typecheck":  "tsc --noEmit",
                    "dev":  "rollup -c rollup.config.js --watch",
                    "clean":  "rimraf dist/",
                    "prepare":  "npm run build"
                },
    "keywords":  [
                     "audio",
                     "music",
                     "chord-analysis",
                     "difference-tones",
                     "harmony",
                     "signal-processing",
                     "t0-theory",
                     "time-mass-duality",
                     "harmonic-analysis",
                     "music-theory"
                 ],
    "author":  {
                   "name":  "Johann Pascher",
                   "url":  "https://github.com/jpascher"
               },
    "repository":  {
                       "type":  "git",
                       "url":  "https://github.com/jpascher/T0-Time-Mass-Duality.git",
                       "directory":  "t0-harmonic-library"
                   },
    "bugs":  {
                 "url":  "https://github.com/jpascher/T0-Time-Mass-Duality/issues"
             },
    "homepage":  "https://github.com/jpascher/T0-Time-Mass-Duality/tree/main/t0-harmonic-library",
    "license":  "MIT",
    "devDependencies":  {
                            "@rollup/plugin-babel":  "^6.0.0",
                            "@rollup/plugin-commonjs":  "^25.0.0",
                            "@rollup/plugin-node-resolve":  "^15.0.0",
                            "@rollup/plugin-terser":  "^0.4.0",
                            "eslint":  "^8.42.0",
                            "jest":  "^29.5.0",
                            "jest-environment-jsdom":  "^29.5.0",
                            "rimraf":  "^5.0.0",
                            "rollup":  "^3.25.0",
                            "typescript":  "^5.1.0"
                        },
    "engines":  {
                    "node":  "\u003e=14.0.0"
                },
    "type":  "module"
}
