    \item Time ($t_P$) -- $1$ -- $\SI{5.391e-44}{\second}$
    \item Mass ($m_P$) -- $1$ -- $\SI{2.176e-8}{\kilogram}$
    \item Energy ($E_P$) -- $1$ -- $\SI{1.220e19}{\giga\electronvolt}$
    \item Temperature ($T_P$) -- $1$ -- $\SI{1.417e32}{\kelvin}$
    \item \text{Schrödinger equation:} \quad -- i\frac{\partial\psi}{\partial t} = H\psi
    \item \text{Uncertainty principle:} \quad -- \Delta E \Delta t \geq \frac{1}{2}
    \item \text{de Broglie relation:} \quad -- \lambda = \frac{1}{p}
    \item \text{Mass-energy:} \quad -- E = m
    \item \text{Energy-momentum:} \quad -- E^2 = p^2 + m^2
    \item \text{Lorentz factor:} \quad -- \gamma = \frac{1}{\sqrt{1-v^2}}
    \item \text{Einstein equations:} \quad -- G_{\mu\nu} = 8\pi T_{\mu\nu}
    \item \text{Schwarzschild radius:} \quad -- r_s = 2M
    \item \text{Coulomb's law:} \quad -- F = \frac{q_1 q_2}{4\pi r^2}
    \item \text{Fine structure constant:} \quad -- \alpha = \frac{e^2}{4\pi}
    \item \text{Stefan-Boltzmann:} \quad -- j = \sigma T^4
    \item \text{Wien's law:} \quad -- \lambda_{max} T = b
    \item \text{Boltzmann distribution:} \quad -- P \propto e^{-E/T}
    \item \textbf{Force} -- \textbf{Dimensionless Coupling} -- \textbf{Typical Value} -- \textbf{Range}
    \item Electromagnetic -- $\alpha_{\text{EM}}$ -- $\sim 1/137$ -- $\infty$
    \item Strong -- $\alpha_s$ -- $\sim 0.118$ at $Q^2 = M_Z^2$ -- $\sim \SI{1e-15}{\meter}$
    \item Weak -- $\alpha_W = g^2/(4\pi)$ -- $\sim 1/30$ -- $\sim \SI{1e-18}{\meter}$
    \item Gravitation -- $\alpha_G = G m^2/(\hbar c)$ -- $m^2/m_P^2$ -- $\infty$
    \item \textbf{SI Unit} -- \textbf{SI Dimension} -- \textbf{Natural Dimension} -- \textbf{Conversion} -- \textbf{Accuracy}
    \item Meter -- $[L]$ -- $[E^{-1}]$ -- $\SI{1}{\meter} \leftrightarrow (\SI{197}{\mega\electronvolt})^{-1}$ -- $< 0.001\%$
    \item Second -- $[T]$ -- $[E^{-1}]$ -- $\SI{1}{\second} \leftrightarrow (\SI{6.58e-22}{\mega\electronvolt})^{-1}$ -- $< 0.00001\%$
    \item Kilogram -- $[M]$ -- $[E]$ -- $\SI{1}{\kilogram} \leftrightarrow \SI{5.61e26}{\mega\electronvolt}$ -- $< 0.001\%$
    \item Ampere -- $[I]$ -- $[E]^{1/2}$ -- $\SI{1}{\ampere} \leftrightarrow (\SI{6.24e18}{\electronvolt})^{1/2}/\si{\second}$ -- $< 0.005\%$
    \item Kelvin -- $[\Theta]$ -- $[E]$ -- $\SI{1}{\kelvin} \leftrightarrow \SI{8.62e-5}{\electronvolt}$ -- $< 0.01\%$
    \item Volt -- $[ML^2 T^{-3} I^{-1}]$ -- $[E]$ -- $\SI{1}{\volt} \leftrightarrow \SI{1}{\electronvolt}/e$ -- $< 0.0001\%$
    \item Coulomb -- $[T I]$ -- $[E^0]$ -- $\SI{1}{\coulomb} \leftrightarrow 6.24 \times 10^{18} \, e$ -- $< 0.0001\%$
    \item John Wiley \& Sons, New York, 1957.
    \item 3rd edition, John Wiley \& Sons, 1998.
\end{itemize}
