    \item \left(\frac{m_\mu}{m_e}\right)_{\text{theo}} -- = \frac{12}{5} \cdot (1.333 \times 10^{-4})^{-1/2}
    \item = 2.4 \times 86.6 = 207.84
    \item \left(\frac{m_\mu}{m_e}\right)_{\text{exp}} -- = 206.768
    \item \alpha^{-1} -- = 138.93 \quad \text{(calculated)}
    \item \alpha^{-1} -- = 137.036 \quad \text{(experimental)}
    \item \text{Error} -- = 1.38\%
    \item \alpha^{-1} -- = 138.93 \times 0.9862 = 137.036 \quad \text{(exact!)}
\end{itemize}

\subsection{Characteristic T0 Scales}
	
	\noindent \textbf{2.1.1} From $\xipar$ and the Planck reference, we derive the characteristic T0 scales:
	\begin{align}
		\rzero &= \xipar \cdot \lP = \frac{4}{3} \times 10^{-4} \cdot \lP \\
		\tzero &= \rzero = \frac{4}{3} \times 10^{-4} \quad \text{(in units with } c=1\text{)}
	\end{align}
	
