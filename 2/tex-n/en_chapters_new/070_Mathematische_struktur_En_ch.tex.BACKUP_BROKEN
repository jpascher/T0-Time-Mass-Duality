    \item $c_\mu$ -- $1.03 \times 10^{20}$ -- 98.1
    \item $\xi$ -- $1.33 \times 10^{-4}$ -- $1.33 \times 10^{-4}$
    \item c_e -- = \frac{3\sqrt{3}}{2\pi\alpha^{1/2}} \quad \textcolor{deepblue}{\text{(Electron coefficient)}}
    \item c_\mu -- = \frac{9}{4\pi\alpha} \quad \textcolor{deepblue}{\text{(Muon coefficient)}}
    \item m_e -- \sim \xi^{5/2} \text{ (Electron)}
    \item m_\mu -- \sim \xi^2 \text{ (Muon)}
    \item \xi -- = \frac{4}{3} \times 10^{-4}
    \item m_e -- = c_e \cdot \xipar^{5/2}
    \item m_\mu -- = c_\mu \cdot \xipar^2
    \item E_0 -- = \sqrt{m_e \cdot m_\mu} = \sqrt{c_e \cdot \xipar^{5/2} \cdot c_\mu \cdot \xipar^2}
    \item = \sqrt{c_e c_\mu} \cdot \xipar^{9/4}
    \item \alpha -- = \xipar \cdot E_0^2 = \xipar \cdot \left[ \sqrt{c_e c_\mu} \cdot \xipar^{9/4} \right]^2
    \item = \xipar \cdot c_e c_\mu \cdot \xipar^{9/2}
    \item = c_e c_\mu \cdot \xipar^{11/2}
    \item m_e -- = c_e \cdot \xi^{5/2}
    \item m_\mu -- = c_\mu \cdot \xi^2
    \item m_\tau -- = c_\tau \cdot \xi^{3/2}
    \item m_e -- = c_e \cdot \xi^{5/2}
    \item = \frac{3\sqrt{3}}{2\pi\alpha^{1/2}} \cdot \left(\frac{4}{3} \times 10^{-4}\right)^{5/2}
    \item = 0.5109989461 \text{ MeV}
    \item m_\mu -- = c_\mu \cdot \xi^2
